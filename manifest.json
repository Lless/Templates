{
    "manifest_version": 2,
    "name": "Templates",
    "version": "0.02",
    "description": "Allows to replace text shortcut with saved template by pressing tab button",

    "icons": {
        "128": "img/icon128.png",
        "48": "img/icon48.png",
        "16": "img/icon16.png"
    },

    "permissions": [],

    "content_scripts" : [
        {
            "matches" : ["<all_urls>"],
            "js" : ["js/libs/mousetrap.changed.js", "js/libs/mousetrap-global-bind.js", "js/contentScript.js"]
        }
    ]
}